<?xml version="1.0" encoding="UTF-8"?>
<aiml>

<category><pattern>*</pattern>
<template><set name="parsedsentence"/>

<srai>PREPROCESS <star/></srai></template>
</category>
<category><pattern>PREPROCESS *</pattern>
<template><think>
    <set name="tempsentence"><get name="parsedsentence"/></set>
    <set name="inputsentence"><star/></set>
    <set name="wordcount"><srai>WORDCOUNTER <star/></srai></set>


    <condition name="wordcount">
        <li value="1">
            
            <set name="testword"><srai>STOPWORD <get name="inputsentence"/> <get name="inputsentence"/></srai></set>
            <condition name="testword">
                <li value="true"> </li>
                <li value="false"><set name="parsedsentence"><get name="tempsentence"/> <get name="inputsentence"/> </set></li>
            </condition>
            
        </li>
        <li> 
            <set name="testword"><srai>STOPWORD <first><get name="inputsentence"/></first> <first><get name="inputsentence"/></first></srai></set>
            <condition name="testword">
                <li value="true"> <set name="parsedsentence"><get name="tempsentence"/> </set></li>
                <li value="false"><set name="parsedsentence"><get name="tempsentence"/> <first><get name="inputsentence"/></first> </set></li>
            </condition>
            <srai>PREPROCESS <rest><get name="inputsentence"/></rest></srai>
        </li>
    </condition>
</think>

PREPROCESSED RESULT:  <get name="parsedsentence"/></template>
</category>
<category><pattern>CLEARPARSEDSENTENCE</pattern>
<template><set name="parsedsentence"/></template>
</category>
</aiml>
